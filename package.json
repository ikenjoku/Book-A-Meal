{
    "name": "book-a-meal",
    "version": "1.0.0",
    "description": "A simple food vendor app",
    "main": "app.js",
    "engines": {
        "node": "8.9.4",
        "npm": "5.6.0"
    },
    "scripts": {
        "start": "babel-node server/bin/www.js",
        "start:client": "webpack-dev-server --mode development --hot --open",
        "start:dev": "nodemon server/bin/www.js --exec babel-node",
        "server:lint": "eslint \"server/**/*.js\" app.js --fix",
        "coverage": "NODE_ENV=test nyc report --reporter=text-lcov | coveralls",
        "test": "npm run test:server",
        "test:server": "cross-env NODE_ENV=test nyc mocha --timeout 10000 server/test/**/*.spec.js  --exit --recursive --require babel-core/register --require regenerator-runtime/runtime",
        "cleardb": "cross-env NODE_ENV=test sequelize db:migrate:undo:all",
        "pretest": "npm run cleardb && cross-env NODE_ENV=test sequelize db:migrate && cross-env NODE_ENV=test sequelize db:seed:all",
        "reset:db": "NODE_ENV=development sequelize db:migrate && sequelize db:seed:all"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ikenjoku/Book-A-Meal.git"
    },
    "author": "Ike Njoku",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/ikenjoku/Book-A-Meal/issues"
    },
    "homepage": "https://github.com/ikenjoku/Book-A-Meal#readme",
    "dependencies": {
        "babel-cli": "^6.26.0",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "babel-polyfill": "^6.26.0",
        "bcrypt": "^2.0.1",
        "body-parser": "^1.18.2",
        "chai": "^4.1.2",
        "debug": "^3.1.0",
        "dotenv": "^5.0.1",
        "express": "^4.16.3",
        "express-validation": "^1.0.2",
        "html-webpack-plugin": "^3.2.0",
        "joi": "^13.2.0",
        "jsonwebtoken": "^8.2.1",
        "moment": "^2.22.1",
        "morgan": "^1.9.0",
        "node-sass": "^4.9.0",
        "pg": "^7.4.1",
        "pg-hstore": "^2.3.2",
        "react": "^16.4.0",
        "react-dom": "^16.4.0",
        "react-redux": "^5.0.7",
        "react-router-dom": "^4.2.2",
        "sequelize": "^4.37.6",
        "swagger-ui": "^3.15.0",
        "swagger-ui-dist": "^3.15.0"
    },
    "devDependencies": {
        "babel-eslint": "^8.2.3",
        "babel-loader": "^7.1.4",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-preset-env": "^1.7.0",
        "babel-preset-react": "^6.24.1",
        "babel-preset-stage-2": "^6.24.1",
        "babel-register": "^6.26.0",
        "chai-http": "^4.0.0",
        "coveralls": "^3.0.0",
        "cross-env": "^5.1.5",
        "css-loader": "^0.28.11",
        "eslint": "^4.9.0",
        "eslint-config-airbnb": "^16.1.0",
        "eslint-config-airbnb-base": "^12.1.0",
        "eslint-plugin-import": "^2.7.0",
        "eslint-plugin-jsx-a11y": "^6.0.2",
        "eslint-plugin-react": "^7.4.0",
        "file-loader": "^1.1.11",
        "mini-css-extract-plugin": "^0.4.0",
        "mocha": "^5.1.1",
        "nodemon": "^1.17.3",
        "nyc": "^11.7.1",
        "sass-loader": "^7.0.1",
        "sinon": "^4.5.0",
        "sinon-chai": "^3.0.0",
        "sinon-express-mock": "^2.0.0",
        "url-loader": "^1.0.1",
        "webpack": "^4.9.1",
        "webpack-cli": "^2.1.4",
        "webpack-dev-server": "^3.1.4"
    }
}
