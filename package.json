    "bugs": {
        "url": "https://github.com/ikenjoku/Book-A-Meal/issues"
    },
    "homepage": "https://github.com/ikenjoku/Book-A-Meal#readme",
    "dependencies": {
        "babel-cli": "^6.26.0",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "babel-polyfill": "^6.26.0",
        "babel-preset-env": "^1.6.1",
        "bcrypt": "^2.0.1",
        "bcrypt-promise": "^2.0.0",
        "body-parser": "^1.18.2",
        "chai": "^4.1.2",
        "debug": "^3.1.0",
        "dotenv": "^5.0.1",
        "express": "^4.16.3",
        "joi": "^13.2.0",
        "jsonwebtoken": "^8.2.1",
        "morgan": "^1.9.0",
        "parse-error": "^0.2.0",
        "passport": "^0.3.2",
        "passport-jwt": "^3.0.0",
        "pg": "^7.4.1",
        "pg-hstore": "^2.3.2",
        "sequelize": "^4.37.6",
        "shortcode": "0.0.0",
        "validator": "^9.2.0"
    },
    "devDependencies": {
        "babel-eslint": "^8.2.3",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-register": "^6.26.0",
        "chai-http": "^4.0.0",
        "coveralls": "^3.0.0",
        "eslint": "^4.9.0",
        "eslint-config-airbnb": "^16.1.0",
        "eslint-config-airbnb-base": "^12.1.0",
        "eslint-plugin-import": "^2.7.0",
        "eslint-plugin-jsx-a11y": "^6.0.2",
        "eslint-plugin-react": "^7.4.0",
