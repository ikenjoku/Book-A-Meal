{
    "name": "book-a-meal",
    "version": "1.0.0",
    "description": "A simple food vendor app",
    "main": "app.js",
    "engines": {
        "node": "8.9.4",
        "npm": "5.6.0"
      },
    "scripts": {
        "start": "babel-node server/bin/www.js",
        "start:dev": "nodemon server/bin/www.js --exec babel-node",
        "test": "NODE_ENV=test nyc mocha server/tests --exit --recursive --require babel-core/register --require regenerator-runtime/runtime",
        "server:lint": "eslint \"server/**/*.js\" app.js --fix",
        "coverage": "NODE_ENV=test nyc report --reporter=text-lcov | coveralls"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ikenjoku/Book-A-Meal.git"
    },
    "author": "Ike Njoku",
